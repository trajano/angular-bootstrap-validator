{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "angular"
  ],
  "extends": [
    "eslint:recommended",
    "angular"
  ],
  "env": {
    "angular/angular": true,
    "browser": true
  },
  "rules": {
    "angular/angularelement": 0,
    "brace-style": 2,
    "camelcase": 2,
    "consistent-return": 2,
    "comma-style": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-extra-parens": 2,
    "no-unused-vars": 2,
    "prefer-arrow-callback": 2,
    "valid-jsdoc": 2,
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "dot-location": [
      2,
      "property"
    ]
  }
}
